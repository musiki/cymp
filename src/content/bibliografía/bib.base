filters:
  and:
    - file.folder == "06-out/bibliograf√≠a"
    - file.ext != "base"
    - file.ext != "bib"
    - '!file.name.contains("MOC")'
views:
  - type: cards
    name: bib gal
    order:
      - file.name
      - author
      - publish
    sort:
      - property: file.ctime
        direction: DESC
    cardSize: 140
    image: note.coverUrl
    imageAspectRatio: 1.55
