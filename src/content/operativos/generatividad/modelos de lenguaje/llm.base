filters:
  and:
    - file.folder == "06-out/operativos/generatividad/modelos de lenguaje"
    - file.ext != "base"
views:
  - type: cards
    name: cards
    order:
      - file.name
      - year
      - embedding_dim
      - max_tokens
      - authors
      - architecture
      - institution
      - fine_tuning
      - city
      - params_million
      - output_format
      - input_format
      - sponsor
      - props_
      - use_cases
      - training_size
      - training_data
      - tech_innovation
    sort:
      - property: year
        direction: ASC
    image: note.img
    imageFit: ""
    imageAspectRatio: 0.4
    cardSize: 270
