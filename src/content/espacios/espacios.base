filters:
  and:
    - file.folder == "06-out/espacios"
    - file.ext != "base"
views:
  - type: cards
    name: Table
    order:
      - file.name
      - year
      - person
      - city
    sort:
      - property: year
        direction: ASC
    image: note.img
    cardSize: 230
    imageAspectRatio: 0.55
