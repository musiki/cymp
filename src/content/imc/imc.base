views:
  - type: cards
    name: IMC gal
    filters:
      and:
        - file.folder == "06-out/imc"
        - '!file.name.contains("MOC")'
        - file.ext != "base"
    order:
      - file.name
      - year
      - sinopsis
      - person
    image: note.img
    cardSize: 250
    imageAspectRatio: 0.45
